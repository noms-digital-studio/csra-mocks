{
  "request": {
    "method": "GET",
    "urlPattern": "/offender/(.*[^a-zA-Z0-9].*)/viper",
    "headers": {
      "Ocp-Apim-Subscription-Key": {
        "equalTo": "valid-subscription-key"
      }
    }
  },
  "response": {
    "status": 409,
    "bodyFileName": "invalid-argument-viper.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}