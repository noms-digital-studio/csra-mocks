{
  "request": {
    "method": "GET",
    "urlPattern": "/elite2api/users/me",
    "headers": {
      "Authorization": {
        "matches": "Bearer ([\\w\\d]+\\.)([\\w\\d]+\\.)([\\w\\d]+)"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "user-me-response.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}